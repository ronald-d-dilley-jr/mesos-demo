
  "id": "/mesos-demo/workmanager",
  "cmd": "work-manager.py",
  "cpus": 0.2,
  "mem": 512,
  "disk": 0,
  "instances": 0,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": false,
      "image": "mesos-demo/work-manager:0.0.1.0",
      "parameters": [],
      "privileged": false
    },
    "volumes": [],
    "portMappings": [
      {
        "containerPort": 0,
        "labels": {},
        "name": "default",
        "protocol": "tcp",
        "servicePort": 10002
      }
    ]
  },
  "env": {
    "DEMO_MSG_SERVICE_CONNECTION_STRING": "amqp://admin:admin-123@messageservice_rabbit_1.rabbit:5672",
